{
  "version": "2.0",
  "extensions": {
    "http": {
      "routePrefix": "",
      "cors": {
        "allowedOrigins": [
          "https://alexjacob.dev",
          "http://localhost:3000",
          "http://127.0.0.1:5500"
        ],
        "allowedMethods": ["GET", "POST", "PUT", "DELETE", "OPTIONS"],
        "allowedHeaders": ["Content-Type", "Authorization"],
        "allowCredentials": true
      }
    }
  },
  "logging": {
    "applicationInsights": {
      "samplingSettings": {
        "isEnabled": true,
        "excludedTypes": "Request"
      }
    }
  }
}